  # Access keys
  'ak_edit'          =&gt; 'e',     # edit page
  'ak_history'       =&gt; 'h',     # page history
  'ak_recentchanges' =&gt; 'c',     # Recent Changes
  'ak_save'          =&gt; 's',     # save page
  'ak_saveedit'      =&gt; 'u',     # save and keep editing
  'ak_preview'       =&gt; 'p',     # preview page
